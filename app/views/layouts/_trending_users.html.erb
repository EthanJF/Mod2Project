<h3>Trending Users</h3>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Username</th>
      <th scope="col">Battles Won</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @users.map do |user| %>

      <td><%= link_to user.username, user %></td>

      <td><%= user.battles_won.count %>

      </td>
    </tr>
    <% end %>
  </tbody>
</table>
